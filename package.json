{
  "name": "vehicle-commission-agents",
  "description": "A desktop app for vehicle sale/purchase business",
  "version": "1.0.0",
  "author": "Mehtab Butt",
  "private": true,
  "engine": "node 16.20.0",
  "scripts": {
    "preinstall": ". ~/.nvm/nvm.sh && nvm install 16.20.0",
    "prestart": ". ~/.nvm/nvm.sh && nvm use 16.20.0",
    "preserve": "yarn --cwd ../server/ start &",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "release": "electron-builder --publish always",
    "build:win32": "electron-builder --win"
  },
  "main": "background.js",
  "dependencies": {
    "@kyvg/vue3-notification": "^2.9.1",
    "ag-grid-community": "^28.2.1",
    "ag-grid-vue3": "^28.2.1",
    "animate.css": "^4.1.1",
    "bcrypt": "^5.1.0",
    "core-js": "^3.8.3",
    "electron-builder-squirrel-windows": "^24.6.3",
    "electron-rebuild": "^3.2.9",
    "electron-squirrel-startup": "^1.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lodash": "^4.17.21",
    "node-ipc": "^11.1.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.25.5",
    "sqlite3": "^5.1.2",
    "tiny-emitter": "^2.1.0",
    "vue": "^3.2.13",
    "vue-router": "4",
    "vuex": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@electron-forge/cli": "^6.2.1",
    "@electron-forge/maker-deb": "^6.2.1",
    "@electron-forge/maker-rpm": "^6.2.1",
    "@electron-forge/maker-squirrel": "^6.2.1",
    "@electron-forge/maker-zip": "^6.2.1",
    "@electron-forge/plugin-auto-unpack-natives": "^6.2.1",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "autoprefixer": "^10.4.13",
    "electron": "^13.0.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.28.0",
    "eslint-plugin-vue": "^9.7.0",
    "postcss": "^8.4.19",
    "prettier": "2.8.3",
    "tailwindcss": "^3.2.4",
    "vue-cli-plugin-electron-builder": "~2.1.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
